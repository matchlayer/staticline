    "pug-lint": "~2.6.0",
    "pug-lint-config-rowupper": "~0.0.1",
    "stylelint": "~13.6.1",
    "stylelint-config-rowupper": "~0.0.1"
  },
  "dependencies": {
    "beeper": "~2.0.0",
    "browser-sync": "~2.26.12",
    "chalk": "~4.1.0",
    "commander": "~6.1.0",
    "cssnano": "~4.1.10",
    "del": "~5.1.0",
    "fancy-log": "~1.3.3",
    "gulp": "~4.0.2",
    "gulp-cached": "~1.1.1",
    "gulp-data": "~1.3.1",
    "gulp-htmlmin": "~5.0.1",
    "gulp-if": "~3.0.0",
